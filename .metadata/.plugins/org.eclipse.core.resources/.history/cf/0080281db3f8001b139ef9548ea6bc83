package com.syntax.Class10;

import java.util.Scanner;

public class LoopMultiplication {

	public static void main(String[] args) {
	
		
		int b=5;
		
		for (int i=1; i<=10; i++) {
			
			int c=b*i;
			System.out.println(c+"X"+i+" ");
		}

		
		int x;
		Scanner scan=new Scanner(System.in);
		x=scan.nextInt();
		
		for (int i=0; i<=4; i++){
		}
	}

}
