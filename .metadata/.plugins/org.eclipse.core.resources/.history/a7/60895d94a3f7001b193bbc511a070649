package syntax.comClass009;

public class TaskForArrays {

	public static void main(String[] args) {
		
		char[] a=new char[6];
		
		a[0]='A';
		a[1]='B';
		a[2]='C';
		a[3]='D';
		a[4]='E';
		a[5]='F';
		
		System.out.println(a[1]);

		
		String[] gNames=new String[6];
		
		gNames[0]="Sharif";
		gNames[1]="Ehsan";
		gNames[2]="Javid";
		gNames[3]="Noor";
		gNames[4]="Bilal the Winner";
		gNames[5]="Jacline";
		
		System.out.println(gNames[0]);
		
	}

}
