package syntax.comClass009;

public class AdvanceLoop {

	public static void main(String[] args) {
		
		for (int i=1; i<=5; i++) {
			System.out.println(i);
		
		for (int j=1; j<4; j++) {
			System.out.println("Loop J");
		}
		}
System.out.println("----MULTIPLICATION TABLE----");
	

	for (int a=1; a<=10; a++) {
		
		for (int b=1; b<=10; b++) {
			
			System.out.println(a+" X "+b+" = "+(a*b));
			
			
			
		}
	}
	/// How the clock starts? here is the Example in loop
	
	for (int c=0; c<=24; c++) {
		
		for (int d=0; d<=24; d++) {
			System.out.println(" Time "+(c*d));
		}
	}
	
	}

}
