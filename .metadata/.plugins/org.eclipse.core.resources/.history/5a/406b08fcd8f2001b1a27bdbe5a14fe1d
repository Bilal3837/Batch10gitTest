package com.syntaxClass008;

public class BreakKeyWord {

	public static void main(String[] args) {
		
		for (int i = 1; i < 6; i++) {
			
			System.out.println("Today is Saturday");
			break;
		}
		boolean summer=true;
		int temp=75;
		
		while (summer) {
		System.out.println(" i Love summer");
	
		if (temp>95)
		
	System.out.println("i don't love summer");	
		break;
		
		}
		temp++;
	}

}
