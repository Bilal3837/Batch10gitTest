package com.syntaxClass008;

public class NestedLoops {

	public static void main(String[] args) {
		
		
		for (int i=1; i<=3; i++);{

				System.out.println("i am i loop");
		
		for (int j = 0; j<=3; j++) {
			
			System.out.println("HOw many times");
		}
		System.out.println("---------");
		}
		
	System.out.println("""""---"""""");
	
	}

}
