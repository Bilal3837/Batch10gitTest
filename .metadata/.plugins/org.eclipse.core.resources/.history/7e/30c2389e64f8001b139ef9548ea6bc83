package com.syntax.Class10;

public class Task2 {

	public static void main(String[] args) {
		
		
		String[] countries= {"Afghanistan", "Morroco", "USA", "Meldiv"};
		
		System.out.println(countries.length);
		
		for (int i=0; i<countries.length; i++) {
			
			System.out.println(i);
			
		}
		
		

	}

}
